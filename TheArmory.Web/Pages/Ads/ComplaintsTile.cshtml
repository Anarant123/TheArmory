@model TheArmory.Domain.Models.Responce.Result.BaseResult.BaseQueryResult<TheArmory.Domain.Models.Responce.ViewModels.Complaint.ComplaintViewModel>

<div class="container">
    <div class="row">
        @foreach (var complaint in Model.Items)
        {
            <div class="col-md-4">
                <div class="card mb-4">
                    <div class="card-body">
                        <h5 class="card-title">@complaint.UserName</h5>
                        <p class="card-text">@complaint.Description</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
